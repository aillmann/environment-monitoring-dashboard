extends layout

block head
	script(type='text/javascript', src='/javascripts/index.js')

block content
	.row
		.col-xs-6
			h3 Reports
			ul
				each val in files
					li
						if val == '*serenity*' 
							a(href='/' + val + '/index.html', target="_blank") #{val}
						else
							a(href='/' + val, target="_blank") #{val}